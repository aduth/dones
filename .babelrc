{
	"presets": [
		[ "@babel/env", {
			"modules": false,
			"targets": {
				"browsers": "last 1 Chrome version"
			}
		} ]
	],
	"plugins": [
		"lodash",
		"@babel/transform-runtime",
		"@babel/proposal-class-properties",
		[ "@wordpress/babel-plugin-import-jsx-pragma", {
			"scopeVariable": "h",
			"scopeVariableFrag": "Fragment",
			"source": "preact",
			"isDefault": false
		} ],
		[ "@babel/transform-react-jsx", {
			"pragma": "h",
			"pragmaFrag": "Fragment"
		} ]
	],
	"env": {
		"test": {
			"presets": [
				[ "@babel/env", {
					"targets": {
						"node": "current"
					}
				} ]
			]
		},
		"production": {
			"presets": [
				[ "@babel/env", {
					"modules": false,
					"targets": {
						"esmodules": true
					},
					"bugfixes": true
				} ]
			]
		},
		"legacy": {
			"plugins": [
				[ "@babel/transform-runtime", {
					"corejs": 2
				} ]
			],
			"presets": [
				[ "@babel/env", {
					"modules": false,
					"targets": {
						"browsers": [
							"last 2 versions",
							"ie 11"
						]
					}
				} ]
			]
		}
	}
}

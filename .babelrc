{
	"presets": [
		[ "@babel/env", {
			"modules": false,
			"targets": {
				"browsers": "last 1 Chrome version"
			}
		} ]
	],
	"plugins": [
		"lodash",
		"@babel/transform-runtime",
		"@babel/proposal-class-properties",
		[ "@babel/transform-react-jsx", {
			"pragma": "createElement",
			"pragmaFrag": "Fragment"
		} ]
	],
	"env": {
		"test": {
			"presets": [
				[ "@babel/env", {
					"targets": {
						"node": "current"
					}
				} ]
			]
		},
		"production": {
			"presets": [
				[ "@babel/env", {
					"modules": false,
					"targets": {
						"browsers": [
							"Chrome >= 61",
							"Safari >= 11",
							"iOS >= 11",
							"Edge >= 16"
						]
					}
				} ]
			]
		},
		"legacy": {
			"plugins": [
				[ "@babel/transform-runtime", {
					"corejs": 2
				} ]
			],
			"presets": [
				[ "@babel/env", {
					"modules": false,
					"targets": {
						"browsers": [
							"last 2 versions",
							"ie 11"
						]
					}
				} ]
			]
		}
	}
}

{
	"presets": [
		[ "env", {
			"modules": false,
			"targets": {
				"browsers": [
					"last 2 versions",
					"ie 11"
				]
			}
		} ],
		"stage-2"
	],
	"plugins": [
		"date-fns",
		"lodash",
		"transform-runtime",
		"transform-react-display-name",
		[ "transform-react-jsx", {
			"pragma": "h"
		} ]
	]
}

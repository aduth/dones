{
  "name": "dones",
  "version": "1.0.4",
  "private": true,
  "description": "WordPress theme for simple team task management and tracking",
  "author": {
    "name": "Andrew Duthie",
    "email": "andrew@andrewduthie.com",
    "url": "http://andrewduthie.com"
  },
  "license": "GPL-2.0+",
  "scripts": {
    "clean": "rimraf dist && mkdir -p dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "build-theme": "./bin/build-theme.sh",
    "lint": "eslint src",
    "predev": "npm run clean",
    "dev": "webpack --watch",
    "unit-test": "cross-env NODE_PATH=src NODE_ENV=test mocha 'src/**/test/*.js' -r mocha.config.js -r babel-register",
    "test": "npm run unit-test && npm run lint"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-date-fns": "0.1.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-react-display-name": "6.25.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "browser-env": "3.0.2",
    "chai": "4.0.2",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "deep-freeze": "0.0.1",
    "eslint": "4.1.1",
    "eslint-config-wpcalypso": "0.8.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-wpcalypso": "3.4.0",
    "extract-text-webpack-plugin": "2.1.2",
    "mocha": "3.4.2",
    "node-sass": "4.5.3",
    "postcss-critical-css": "3.0.0",
    "postcss-loader": "2.0.6",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.0.0"
  },
  "dependencies": {
    "autosize": "3.0.21",
    "classnames": "2.2.5",
    "date-fns": "1.28.5",
    "fast-stable-stringify": "0.2.0",
    "flatpickr": "3.0.6",
    "lodash": "4.17.4",
    "moize": "2.4.1",
    "path-to-regexp": "1.7.0",
    "preact": "8.1.0",
    "preact-redux": "2.0.2",
    "querystringify": "1.0.0",
    "redux": "3.7.1",
    "redux-multi": "0.1.12",
    "refx": "2.0.0",
    "rememo": "1.1.1",
    "shallow-equal": "1.0.0",
    "textarea-caret": "3.0.2"
  }
}
